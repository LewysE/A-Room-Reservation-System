@using Microsoft.AspNetCore.Identity

@inject SignInManager<IdentityUser> SignInManager
@inject UserManager<IdentityUser> UserManager

@if (User.Identity.IsAuthenticated)
{
<ul class="nav navbar-nav navbar-right">
    <li class="nav-item"><a class="nav-link" asp-controller="Account" asp-action="Logout">Logout</a></li>
    <li class="nav-item"><a class="nav-link" asp-controller="Account" asp-action="Logout">@User.Identity.Name</a></li>
</ul>
}
else
{
    <ul class="nav navbar-nav navbar-right">
       
        <li class="nav-item"><a class="nav-link" id="qsLoginBtn" asp-controller="Account" asp-action="Login">Login/Sign Up</a></li>
    </ul>
}